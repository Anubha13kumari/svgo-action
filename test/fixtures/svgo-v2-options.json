{
  "multipass": true,
  "plugins": [
    {
      "name": "convertPathData",
      "params": {
        "floatPrecision": 3,
        "noSpaceAfterFlags": false
      }
    },
    {
      "name": "sortAttrs",
      "params": {
        "order": [
          "role",
          "viewBox"
        ],
        "xmlnsOrder": "end"
      }
    },
    {
      "name": "convertShapeToPath",
      "params": {
        "convertArcs": true
      }
    },
    {
      "name": "mergePaths",
      "params": {
        "force": true,
        "noSpaceAfterFlags": false
      }
    },
    {
      "name": "removeTitle"
    },
    {
      "name": "addAttributesToSVGElement",
      "params": {
        "attributes": [
          {
            "role": "img"
          }
        ]
      }
    }
  ]
}
